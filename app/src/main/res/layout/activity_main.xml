<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.453"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="CALCULADORA" />

    <TextView
        android:id="@+id/operation"
        android:layout_width="373dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:text="0.0"
        android:textAlignment="textEnd"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/result"
        android:layout_width="371dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="0.0"
        android:textAlignment="textEnd"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/operation" />

    <Button
        android:id="@+id/clean"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginStart="292dp"
        android:layout_marginTop="44dp"
        android:background="#FBFBFB"
        android:text="C"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/result" />

    <Button
        android:id="@+id/seven"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="116dp"
        android:layout_marginBottom="16dp"
        android:background="#F4EBEB"
        android:backgroundTint="#000000"
        android:text="7"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/six"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/result" />

    <Button
        android:id="@+id/eight"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginStart="8dp"
        android:background="#F4F4F4"
        android:text="8"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/seven"
        tools:layout_editor_absoluteY="302dp" />

    <Button
        android:id="@+id/division"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:background="#EFEFEF"
        android:text="/"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="294dp"
        tools:layout_editor_absoluteY="536dp" />

    <Button
        android:id="@+id/equal"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginEnd="24dp"
        android:background="#E3E3E3"
        android:text="="
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/division"
        tools:layout_editor_absoluteY="536dp" />

    <Button
        android:id="@+id/dot"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginEnd="16dp"
        android:background="#E3E3E3"
        android:text="."
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/equal"
        tools:layout_editor_absoluteY="536dp" />

    <Button
        android:id="@+id/zero"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginEnd="8dp"
        android:background="#E4E4E4"
        android:text="0"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/dot"
        tools:layout_editor_absoluteY="536dp" />

    <Button
        android:id="@+id/multiplication"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginStart="24dp"
        android:background="#EFEAEA"
        android:text="X"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/three"
        tools:layout_editor_absoluteY="461dp" />

    <Button
        android:id="@+id/three"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginStart="16dp"
        android:background="#EDEDED"
        android:text="3"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/two"
        tools:layout_editor_absoluteY="461dp" />

    <Button
        android:id="@+id/two"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginStart="8dp"
        android:background="#E4E4E4"
        android:text="2"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/one"
        tools:layout_editor_absoluteY="461dp" />

    <Button
        android:id="@+id/one"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginBottom="16dp"
        android:background="#E8E8E8"
        android:text="1"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/zero"
        tools:layout_editor_absoluteX="45dp" />

    <Button
        android:id="@+id/six"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginEnd="8dp"
        android:background="#ECE4E4"
        android:text="6"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/five"
        tools:layout_editor_absoluteY="379dp" />

    <Button
        android:id="@+id/five"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginEnd="16dp"
        android:background="#EFECEC"
        android:text="5"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/four"
        tools:layout_editor_absoluteY="379dp" />

    <Button
        android:id="@+id/four"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginEnd="24dp"
        android:background="#EFEDED"
        android:text="4"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/rest"
        tools:layout_editor_absoluteY="379dp" />

    <Button
        android:id="@+id/rest"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginBottom="24dp"
        android:background="#EAEAEA"
        android:text="-"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/multiplication"
        tools:layout_editor_absoluteX="294dp" />

    <Button
        android:id="@+id/plus"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginStart="24dp"
        android:background="#EAEAEA"
        android:text="+"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/nine"
        tools:layout_editor_absoluteY="302dp" />

    <Button
        android:id="@+id/nine"
        android:layout_width="67dp"
        android:layout_height="58dp"
        android:layout_marginStart="16dp"
        android:background="#F3EFEF"
        android:text="9"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/eight"
        tools:layout_editor_absoluteY="302dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Creado por:"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="629dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Andres"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="648dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Daniel"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="676dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="David Prieto"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="707dp" />

</androidx.constraintlayout.widget.ConstraintLayout>